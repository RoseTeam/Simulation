<?xml version="1.0" ?>
<sdf version="1.5">
  <!--une balle de ping pong pÃ¨se 2,7 g pour un rayon de 20mm-->
  <model name="popcorn">
    <link name="link">
      <self_collide>1</self_collide>
      <inertial>
		<mass>.0027</mass>
	    <inertia>
			<!--2/3mr^2 pour une sphere creuse-->
            <ixx>0.00000072</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.00000072</iyy>
            <iyz>0</iyz>
            <izz>0.00000072</izz>
        </inertia>
      </inertial>
	  <collision name="collision">
		<max_contacts>1</max_contacts>
        <geometry>
          <sphere>
            <radius>0.02</radius><!-- specification E2015_Rules_EU_EN_final.pdf3 -->
          </sphere>
        </geometry>
		<surface>
		  <bounce>
			<restitution_coefficient>0.9128</restitution_coefficient>
			<threshold>0.001</threshold>
		  </bounce>
          <contact>
            <ode>
              <max_vel>10</max_vel>
            </ode>
          </contact>
		  <friction>
			<ode>
			  <mu>1</mu>
			  <mu2>1</mu2>
			</ode>
		  </friction>
		</surface>
      </collision>
      <visual name="visual">
        <geometry>
          <sphere>
            <radius>0.02</radius>
          </sphere>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/White</name>
          </script>
        </material>
      </visual>
    </link>
    </model>
 </sdf>
