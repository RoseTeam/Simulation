<?xml version="1.0"?>
<sdf version="1.5">
  <model name="Rosebot">
    <link name="chassis">
      <pose>0 0 .105 0 0 0</pose>
      <inertial>
        <mass>5.</mass>
        <inertia>
          <ixx>1</ixx>
          <iyy>1</iyy>
          <izz>0.8</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="chassis_collision">
        <geometry>
          <box>
            <size>0.2 0.3 0.2</size>
          </box>
        </geometry>
      </collision>
      <visual name="chassis_visual">
        <geometry>
          <box>
            <size>0.2 0.3 0.2</size>
          </box>
        </geometry>
      </visual>
	  <visual name="indicateur_avant">
        <pose> .105 0 .105 0 0 0</pose>
	    <geometry>
	      <sphere>
	        <radius>0.015</radius>
	      </sphere>
	    </geometry>
	    <material>
	      <script>
	        <uri>file://media/materials/scripts/gazebo.material</uri>
	        <name>Gazebo/Orange</name>
	      </script>
	    </material>
	  </visual>
    </link>
    <link name="roue_folle_avant">
	  <pose>0.09 0 .01 0 0 0</pose>
      <inertial>
        <mass>.05</mass>
        <inertia>
          <ixx>1</ixx>
          <iyy>1</iyy>
          <izz>1</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
	  <collision name="castor_collision">
	    <geometry>
	      <sphere>
	        <radius>0.015</radius>
	      </sphere>
	    </geometry>
	    <surface>
	      <friction>
	        <ode>
	          <mu>0</mu>
	          <mu2>0</mu2>
	          <slip1>1.0</slip1>
	          <slip2>1.0</slip2>
	        </ode>
	      </friction>
	    </surface>
	  </collision>
	  <visual name="castor_visual">
	    <geometry>
	      <sphere>
	        <radius>0.015</radius>
	      </sphere>
	    </geometry>
	    <material>
	      <script>
	        <uri>file://media/materials/scripts/gazebo.material</uri>
	        <name>Gazebo/FlatBlack</name>
	      </script>
	    </material>
	  </visual>
    </link>
	<link name="roue_folle_arriere">
	  <pose>-0.09 0 .01 0 0 0</pose>
      <inertial>
        <mass>.05</mass>
        <inertia>
          <ixx>1</ixx>
          <iyy>1</iyy>
          <izz>1</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
	  <collision name="castor_collision">
	    <geometry>
	      <sphere>
	        <radius>0.015</radius>
	      </sphere>
	    </geometry>
	    <surface>
	      <friction>
	        <ode>
	          <mu>0</mu>
	          <mu2>0</mu2>
	          <slip1>1.0</slip1>
	          <slip2>1.0</slip2>
	        </ode>
	      </friction>
	    </surface>
	  </collision>
	  <visual name="castor_visual">
	    <geometry>
	      <sphere>
	        <radius>0.015</radius>
	      </sphere>
	    </geometry>
	    <material>
	      <script>
	        <uri>file://media/materials/scripts/gazebo.material</uri>
	        <name>Gazebo/FlatBlack</name>
	      </script>
	    </material>
	  </visual>
    </link>
    <link name="right_wheel">
      <pose>0.0 -.126 0.03 0 1.5707 1.5707</pose>
      <inertial>
        <mass>.1</mass>
        <inertia>
          <ixx>0.0051</ixx>
          <iyy>0.0051</iyy>
          <izz>0.0090</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.035</radius>
            <length>0.036</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.035</radius>
            <length>0.036</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
        </material>
      </visual>
    </link>
    <link name="left_wheel">
      <pose>0.0 .126 0.03 0 1.5707 1.5707</pose>
      <inertial>
        <mass>.1</mass>
        <inertia>
          <ixx>0.0051</ixx>
          <iyy>0.0051</iyy>
          <izz>0.0090</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.035</radius>
            <length>0.036</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.035</radius>
            <length>0.036</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
        </material>
      </visual>
    </link>
    <joint name="attache_roue_folle_avant" type="revolute">
      <child>roue_folle_avant</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
	   <limit>
		<upper>0</upper>
		<lower>0</lower>
	   </limit>
      </axis>
    </joint>
    <joint name="attache_roue_folle_arriere" type="revolute">
      <child>roue_folle_arriere</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
	   <limit>
		<upper>0</upper>
		<lower>0</lower>
	   </limit>
      </axis>
    </joint>
    <joint name="left_wheel_hinge" type="revolute">
      <child>left_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    <joint name="right_wheel_hinge" type="revolute">
      <child>right_wheel</child>
      <parent>chassis</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
	<plugin name="rosebot_gazebo" filename="libgazebo_ros_diff_drive.so">
		<leftJoint>left_wheel_hinge</leftJoint>
		<rightJoint>right_wheel_hinge</rightJoint>
		<wheelAcceleration>0.</wheelAcceleration><!--accel m/s/s valeur de 0 : accel instantanÃ©e-->
	    <wheelDiameter>0.036</wheelDiameter>
		<wheelTorque>.29</wheelTorque><!--Couple: 3 kg.cm pour le moteur gotronic-->
		<updateRate>50</updateRate>
		<robotBaseFrame>chassis</robotBaseFrame>
		<commandTopic>cmd_vel</commandTopic>
    	<odometryTopic>odom</odometryTopic>
		<!--
    	<odometryFrame>chassis</odometryFrame>
		-->
		<publishWheelTF>false</publishWheelTF>
	</plugin>

  </model>
</sdf>
