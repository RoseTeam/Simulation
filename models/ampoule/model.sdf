<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="ampoule">
    <link name="link">
      <!--poids d'une balle de tennis FIT: 56,70 gr Ã  58,5 gr. -->
      <inertial>
		<mass>.058</mass>
	    <inertia>
			<!--2/3mr^2 pour une sphere creuse-->
            <ixx>0.00004339</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.00004339</iyy>
            <iyz>0</iyz>
            <izz>0.00004339</izz>
        </inertia>
      </inertial>
	  <collision name="collision">
		<max_contacts>1</max_contacts>
        <geometry>
          <sphere>
            <radius>0.033</radius><!-- specification E2015_Rules_EU_EN_final.pdf3 -->
          </sphere>
        </geometry>
		<surface>
		  <bounce>
			<restitution_coefficient>0.728</restitution_coefficient>
			<threshold>0.01</threshold>
		  </bounce>
         <contact>
            <ode>
              <max_vel>10</max_vel>
            </ode>
          </contact>
		  <friction>
			<ode>
			  <mu>1</mu>
			  <mu2>1</mu2>
			</ode>
		  </friction>
		</surface>
      </collision>
      <visual name="visual">
        <geometry>
          <sphere>
            <radius>0.0335</radius>
          </sphere>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/Yellow</name>
          </script>
        </material>
      </visual>
    </link>
    </model>
 </sdf>
